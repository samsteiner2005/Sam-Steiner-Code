<br>
<div class="error">
<?php

if(isset($_GET["error"])) {

   if($_GET["error"]=="emptyinput") {
   echo "<p>Enter all infomation!</p>";
   } 
   if ($_GET["error"]=="invalidEmail") {
      echo "<p>The email you have entered is not correct!</p>";

   } if ($_GET["error"]=="emailExists") {
      echo "<p>The email you have entered already exists!</p>";
   }
   if ($_GET["error"]=="school_idExists") {
      echo "<p>The school ID you have entered already exists!</p>";
   }
    if ($_GET["error"]=="stmtfailed") {
      echo "<p>Something isn't working. Try again!</p>";
   }
   if ($_GET["error"]=="school_idTooLong") {
      echo "<p>The school ID you entered is not the right length!</p>";
   }
   if ($_GET["error"]=="none") {
      echo "<p>You have created an account!</p>";
   }
   
   }
?>
</div>
